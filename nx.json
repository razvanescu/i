{
  "npmScope": "stores",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "chara-web-service": {
      "tags": ["application"]
    },
    "chara-web-service-e2e": {
      "tags": [],
      "implicitDependencies": ["chara-web-service"]
    },
    "nx-stores": {
      "tags": ["application"]
    },
    "nx-stores-e2e": {
      "tags": [],
      "implicitDependencies": ["nx-stores"]
    },
    "shared-ui": {
      "tags": []
    }
  }
}
